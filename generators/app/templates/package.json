{
  "name": "<%= appName %>",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "webpack": "webpack --config ./webpack.config.js --progress --colors --profile --bail",
    "watch": "webpack-dev-server --config ./webpack.config.js --progress --colors",
    "tslint": "tslint -c ./tslint.json 'dev/**/*.ts'; exit 0;"<% if(tests == true) { %>,
    "test": "mocha"<% } %>
  },
  "dependencies": {
    "@angular/animations": "^5.1.3",
    "@angular/common": "^5.1.3",
    "@angular/compiler": "^5.1.3",
    "@angular/core": "^5.1.3",
    "@angular/forms": "^5.1.3",
    "@angular/http": "^5.1.3",
    "@angular/platform-browser": "^5.1.3",
    "@angular/platform-browser-dynamic": "^5.1.3",
    "@angular/router": "^5.1.3",
    "core-js": "^2.5.3",
    "fs-extra": "^1.0.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19",<% if(materialDesign == true) { %>
    "@angular/material": "^5.0.3",
    "@angular/cdk": "^5.0.3",
    "material-design-icons": "^3.0.1",<% } %><% if(objectMapping == 'Mongoose') { %>
    "bluebird": "^3.5.1",<% } %>
    "co-body": "^5.1.1",
    "co-views": "^2.1.0",<% if(sqlDialect == 'mariadb') { %>
    "mariasql": "^0.2.6",<% } if(objectMapping == 'Mongoose') { %>
    "mongoose": "^4.13.9",<% } if(sqlDialect == 'mysql') { %>
    "mysql2": "^1.5.1",<% } if(objectMapping == 'Sequelize') { %>
    "sequelize": "^4.29.2",<% } if(sqlDialect == 'postgres') { %>
    "pg": "^7.4.1",<% } %>
    "koa": "^2.4.1",<% if(objectMapping != 'None') { %>
    "koa-bodyparser": "^4.2.0",
    "koa-generic-session": "^2.0.1",
    "koa-passport": "^4.0.1",
    "passport-local": "^1.0.0",<% } %>
    "koa-compress": "^2.0.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.44",
    "@types/node": "^9.3.0",
    "codelyzer": "4.0.2",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",<% if(templateEngine == true) { %>
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",<% } else { %>
    "html-loader": "^0.5.4",<% } %>
    "style-loader": "^0.19.1",<% if(cssPreprocessor == 'Stylus') { %>
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",<% } %><% if(cssPreprocessor == 'Less') { %>
    "less": "^2.7.3",
    "less-loader": "^4.0.5",<% } %><% if(cssPreprocessor == 'Sass') { %>
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",<% } %>
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0",
    "webpack-merge": "^4.1.1"<% if(tests == true) { %>,
    "mocha": "^4.1.0",
    "supertest": "^3.0.0"<% } %>
  }
}
