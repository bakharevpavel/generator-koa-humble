{
  "name": "<%= props.appName %>",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app.js"<% if(props.tests == true) { %>,
    "test": "mocha"<% } %>
  },
  "dependencies": {<% if(props.objectMapping == 'Mongoose') { %>
    "bluebird": "^3.4.0",<% } %>
    "co-body": "^4.2.0",
    "co-views": "^2.1.0",<% if(props.templateEngine == 'Jade') { %>
    "jade": "^1.11.0",<% } if(props.sqlDialect == 'mariadb') { %>
    "mariasql": "^0.2.6",<% } if(props.objectMapping == 'Mongoose') { %>
    "mongoose": "^4.7.6",<% } if(props.sqlDialect == 'mysql') { %>
    "mysql": "^2.12.0", <% } if(props.objectMapping == 'Sequelize') { %>
    "sequelize": "^3.29.0",<% } if(props.templateEngine == 'Swig') { %>
    "swig": "^1.3.2",<% } if(props.sqlDialect == 'postgres') { %>
    "pg": "^6.1.2",<% } if(props.koa == 'Koa v1') { %>
    "koa": "^1.2.4",
    "koa-convert": "^1.2.0",<% } if(props.koa == 'Koa v2') { %>
    "koa": "^2.0.0",<% } if(props.objectMapping != 'None') { %>
    "koa-bodyparser": "^2.2.0",
    "koa-generic-session": "^1.11.4",
    "koa-passport": "^1.4.0",
    "passport-local": "^1.0.0",
    "md5": "^2.1.0",<% } %>
    "koa-compress": "^1.0.9",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-imagemin": "^3.1.1",<% if(props.cssPreprocessor == 'Less') { %>
    "gulp-less": "^3.3.0",<% } %>
    "gulp-rename": "^1.2.2",<% if(props.cssPreprocessor == 'Sass') { %>
    "gulp-sass": "^3.0.0",<% } if(props.cssPreprocessor == 'Stylus') { %>
    "gulp-stylus": "^2.6.0",<% } %>
    "gulp-uglify": "^2.0.0"<% if(props.tests == true) { %>,
    "mocha": "^3.2.0",
    "supertest": "^2.0.1"<% } %>
  }
}
