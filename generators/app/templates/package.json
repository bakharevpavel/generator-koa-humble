{
  "name": "<%= appName %>",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "webpack": "webpack --config ./webpack.config.js --progress --colors --profile --bail",
    "watch": "webpack-dev-server --config ./webpack.config.js --progress --colors",
    "tslint": "tslint -c ./tslint.json 'dev/**/*.ts'; exit 0;"<% if(tests == true) { %>,
    "test": "mocha"<% } %>
  },
  "dependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/platform-browser": "^4.4.5",
    "@angular/platform-browser-dynamic": "^4.4.5",
    "@angular/router": "^4.4.5",
    "core-js": "^2.5.1",
    "fs-extra": "^1.0.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.18",<% if(materialDesign == true) { %>
    "@angular/material": "^2.0.0-beta.12",
    "@angular/cdk": "^2.0.0-beta.12",
    "material-design-icons": "^3.0.1",<% } %><% if(objectMapping == 'Mongoose') { %>
    "bluebird": "^3.4.0",<% } %>
    "co-body": "^4.2.0",
    "co-views": "^2.1.0",<% if(sqlDialect == 'mariadb') { %>
    "mariasql": "^0.2.6",<% } if(objectMapping == 'Mongoose') { %>
    "mongoose": "^4.7.6",<% } if(sqlDialect == 'mysql') { %>
    "mysql": "^2.12.0",<% } if(objectMapping == 'Sequelize') { %>
    "sequelize": "^3.29.0",<% } if(sqlDialect == 'postgres') { %>
    "pg": "^6.1.2",<% } %>
    "koa": "^2.3.0",<% if(objectMapping != 'None') { %>
    "koa-bodyparser": "^4.2.0",
    "koa-generic-session": "^2.0.0",
    "koa-passport": "^4.0.1",
    "passport-local": "^1.0.0",<% } %>
    "koa-compress": "^2.0.0",
    "koa-logger": "^3.0.1",
    "koa-router": "^5.4.1",
    "koa-static": "^4.0.1"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.43",
    "@types/node": "^8.0.47",
    "codelyzer": "4.0.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",<% if(templateEngine == true) { %>
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",<% } else { %>
    "html-loader": "^0.5.1",<% } %>
    "style-loader": "^0.19.0",<% if(cssPreprocessor == 'Stylus') { %>
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",<% } %><% if(cssPreprocessor == 'Less') { %>
    "less": "^2.7.2",
    "less-loader": "^4.0.5",<% } %><% if(cssPreprocessor == 'Sass') { %>
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",<% } %>
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.0"<% if(tests == true) { %>,
    "mocha": "^4.0.1",
    "supertest": "^3.0.0"<% } %>
  }
}
